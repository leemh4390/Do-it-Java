public class ExerciseTest {
	public static void main(String[] args) {
		
		System.out.println("연습문제 1 : " + (2 + 3 / 2)); // 예상 답 : 3
		System.out.println("연습문제 2 : " + (10 % 3 + 5 / 3 )); // 2
		System.out.println("연습문제 3 : " + (4 + 5 / 2) % 4); // 2
		
		
	}
}
